COMP FULL_ADDER ; IN: A, B, CIN ; OUT: S, COUT
BEGIN FULL_ADDER NETLIST
    U1 XOR2 A, B
    U2 XOR2 U1, CIN
    U3 AND2 U1, CIN
    U4 AND2 A, B
    U5 OR2 U3, U4
    S = U2
    COUT = U5
END FULL_ADDER NETLIST

COMP FULL_ADDER_SUBTRACTOR ; IN: A, B, CIN, ADD'_SUB ; OUT: S , COUT 
BEGIN FULL_ADDER_SUBTRACTOR NETLIST
    U1 XOR2 ADD'_SUB, B
    U2 NAND2 A, U1
    U3 XOR2 A, U1
    U4 NAND2 U3, CIN
    U5 XOR2 U3, CIN
    U6 NAND2 U2, U4
    S = U5
    COUT = U6
END FULL_ADDER_SUBTRACTOR NETLIST

COMP MUX ; IN: A, B, C ; OUT: D
BEGIN MUX NETLIST
    U1 NOT C
    U2 AND2 A, U1
    U3 AND2 B, C
    U4 OR2 U2, U3
    D = U4
END MUX NETLIST

COMP MUX_N ; IN: A, B, C ; OUT: D
BEGIN MUX_N NETLIST
    U1 NOT C
    U2 NAND2 A, U1
    U3 NAND2 B, C
    U4 NAND2 U2, U3
    D = U4
END MUX_N NETLIST
