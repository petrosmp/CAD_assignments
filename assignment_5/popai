COMP adder_subtractor ; IN: A2, A1, A0, B2, B1, B0, CIN, ADD_SUB_MODE ; OUT: S2, S1, S0, COUT
BEGIN adder_subtractor NETLIST
U1 FULL_ADDER_SUBTRACTOR A0 B0 CIN ADD_SUB_MODE
U2 FULL_ADDER_SUBTRACTOR A1 B1 U1_COUT ADD_SUB_MODE
U3 FULL_ADDER_SUBTRACTOR A2 B2 U2_COUT ADD_SUB_MODE
S2 = U3_S 
S1 = U2_S 
S0 = U1_S 
COUT = U3_COUT
END adder_subtractor NETLIST
